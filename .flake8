[flake8]
max-line-length = 88
extend-ignore = E203, E266, E501, W503, F403, F401
max-complexity = 10
exclude =
    .git,
    __pycache__,
    venv,
    .venv,
    migrations,
    .tox,
    build,
    dist,
    .eggs,
    *.egg
per-file-ignores =
    __init__.py:F401
    app/config.py:E501
    tests/*:F401,F811
